--fFriction,fRestitution,fDensity
--selfflags,checkmask,flags
--vCenter - orxVector
--fRadius
--mesh
--VertexCount
--Vertices[MESH_VERTEX_NUMBER]
--edge
--Vertices[2] - orxVector
--vPrev - orxVector
--vNext - orxVector
--hasprev - bool, hasnext - bool

--chain
--vPrev - orxVector
--vNext - orxVector
--VertexCount int
--Loop - bool
--hasprev - bool
--hasnext - bool
-----------------------------------------------
--joint def struct
--vSrcScale - orxVector
--vDstScale - orxVector
--vSrcAnhor - orxVector
--vDstAnchor - orxVector

--Revolute
--fDefaultRotation,fMinRot,fMaxRot,fMotorSpeed,fMaxMotorTorque

--Prismatic
--vTranslationAxis - orxVector
--fDefaultRot,fMinTrans,fMaxTrans,fMotoSpeed,fMotorForce

--Spring
--fLength,fFrequency,fDamping

--Rope
--fLength

--Pulley
--vSrcGroundAcnhor - orxVector
--vDstGroundAnchor - orxVector
--fLengthRatio,fSrcLength,fMaxSrcLength,fDstLength,fMacDstLength

--Suspension
