include std/machine.e
include std/math.e
include std/dll.e
include std/os.e
include std/types.e

--global orx var
public atom orx

--Open correct library for OS
ifdef WINDOWS then
	orx = open_dll("orx.dll")
	elsifdef LINUX or FREEBSD then
		orx = open_dll("orx.so")
	if orx = -1 then
		puts(1,"Failed to open orx.dll!\n")
		abort(0)
	end if
end ifdef
			
